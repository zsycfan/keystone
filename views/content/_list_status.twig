<td>
  {% if page.published %}
    <span class="label-group">
      <span class="label label-success">Live</span>
      {% if page.published_id != page.revision_id %}
        <span class="label label-warning">Draft</span>
      {% endif %}
    </span>
  {% else %}
    <span class="label label-warning">Draft</span>
  {% endif %}
</td>